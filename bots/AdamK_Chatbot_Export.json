{
  "metadata": {
    "schemaVersion": "1.0",
    "importType": "LEX",
    "importFormat": "JSON"
  },
  "resource": {
    "name": "AdamK_Chatbot",
    "version": "1",
    "intents": [
      {
        "name": "FindMeal",
        "version": "15",
        "fulfillmentActivity": {
          "type": "CodeHook",
          "codeHook": {
            "uri": "arn:aws:lambda:us-east-1:135393461222:function:chatbot-find-meals-dev-fulfillment",
            "messageVersion": "1.0"
          }
        },
        "sampleUtterances": [
          "I want food",
          "I want a meal near {Intersection}",
          "Where can I find dinner near {Intersection}",
          "I am looking for something to eat near {Intersection} at {Date} {Time}",
          "Can I get something to eat {Date} {Time}",
          "Give me {mealType}",
          "I would like {mealType}",
          "I want a meal on {Date} at {Time}",
          "I want a meal at {Time} on {Date}",
          "I want {mealType} on Friday at {Time}"
        ],
        "slots": [
          {
            "name": "Date",
            "slotConstraint": "Required",
            "slotType": "AMAZON.DATE",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Which day do you want to eat on?"
                }
              ],
              "maxAttempts": 2,
              "responseCard": "{\"version\":1,\"contentType\":\"application/vnd.amazonaws.card.generic\"}"
            },
            "priority": 2,
            "sampleUtterances": []
          },
          {
            "name": "Intersection",
            "slotConstraint": "Optional",
            "slotType": "AMAZON.PostalAddress",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Where are you near?"
                },
                {
                  "contentType": "PlainText",
                  "content": "What intersection are you close to?"
                },
                {
                  "contentType": "PlainText",
                  "content": "Where in the city are you?"
                }
              ],
              "maxAttempts": 2,
              "responseCard": "{\"version\":1,\"contentType\":\"application/vnd.amazonaws.card.generic\"}"
            },
            "priority": 1,
            "sampleUtterances": []
          },
          {
            "name": "Latitude",
            "slotConstraint": "Required",
            "slotType": "AMAZON.NUMBER",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Where are you near?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 5,
            "sampleUtterances": []
          },
          {
            "name": "Longitude",
            "slotConstraint": "Required",
            "slotType": "AMAZON.NUMBER",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "Where are you near?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 6,
            "sampleUtterances": []
          },
          {
            "name": "mealType",
            "slotConstraint": "Optional",
            "slotType": "MealType",
            "slotTypeVersion": "2",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "e.g. What city?"
                }
              ],
              "maxAttempts": 2
            },
            "priority": 4,
            "sampleUtterances": []
          },
          {
            "name": "Time",
            "slotConstraint": "Required",
            "slotType": "AMAZON.TIME",
            "valueElicitationPrompt": {
              "messages": [
                {
                  "contentType": "PlainText",
                  "content": "What time would you like to eat at?"
                },
                {
                  "contentType": "PlainText",
                  "content": "When would you like to eat?"
                }
              ],
              "maxAttempts": 2,
              "responseCard": "{\"version\":1,\"contentType\":\"application/vnd.amazonaws.card.generic\"}"
            },
            "priority": 3,
            "sampleUtterances": []
          }
        ],
        "dialogCodeHook": {
          "uri": "arn:aws:lambda:us-east-1:135393461222:function:chatbot-find-meals-dev-validations",
          "messageVersion": "1.0"
        }
      }
    ],
    "slotTypes": [
      {
        "name": "MealType",
        "version": "2",
        "enumerationValues": [
          {
            "value": "Breakfast"
          },
          {
            "value": "Dinner",
            "synonyms": [
              "Supper"
            ]
          },
          {
            "value": "Lunch"
          },
          {
            "value": "Food",
            "synonyms": [
              "A meal"
            ]
          }
        ],
        "valueSelectionStrategy": "TOP_RESOLUTION"
      }
    ],
    "voiceId": "Salli",
    "childDirected": false,
    "locale": "en-US",
    "idleSessionTTLInSeconds": 600,
    "description": "AdamK's fork of a bot",
    "clarificationPrompt": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "I didn't understand you, what would you like to do?"
        }
      ],
      "maxAttempts": 3
    },
    "abortStatement": {
      "messages": [
        {
          "contentType": "PlainText",
          "content": "Sorry, I'm not able to assist at this time"
        }
      ]
    }
  }
}